#include<iostream>

using namespace std;



int main()
{
    int n;
    cin>>n;
    float *a;
    int i;
    a=(float*)calloc(n,sizeof(float));
    
    if(a[i]==0)
    {
        cout<<"no memory is located";
    }
    for( i=0;i<n;i++)
    {
        cin>>a[n];
        exit(0);
    }
    for(i=0;i<n;i++)
    {
        if(*a<*a+i)
        {
            *a=*a+i;
        }
    }
    cout<<"the largest element is"<<*a;
}
