#include<iostream>

using namespace std;

int main()
{
    int m=29;
    int* ab;
    cout<<&m<<endl;
    cout<<m<<endl;
    
    ab=&m;
    *ab=29;
    cout<<ab<<endl;
    cout<<*ab<<endl;
    
    m=34;
    cout<<ab<<endl;
    cout<<*ab<<endl;
    
    *ab=7;
    m=7;
    cout<<&m<<endl;
    cout<<m<<endl;
    
    
    return 0;
    
}
